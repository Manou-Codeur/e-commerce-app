alse);\r\n\r\n  const { firebase } = useContext(FirebaseContext);\r\n\r\n  //get the card length from redux store\r\n  const cardLength = useSelector(state => {\r\n    let length = 0;\r\n\r\n    for (let product of state.products) {\r\n      length += product.amount;\r\n    }\r\n\r\n    return length;\r\n  });\r\n\r\n  //I wrapper this code in trycatch coz the jwtGenerator() throw an error if the string passed isn't valid jwt code\r\n  try {\r\n    const jwt = jwtGenerator(JSON.parse(localStorage.getItem(\"user-authed\")));\r\n    var userAuthed = jwt.aud === \"react-e-commerce-app-18fea\";\r\n  } catch (error) {}\r\n\r\n  const SingOut_SingIn = () => {\r\n    //sing out\r\n    if (userAuthed) {\r\n      localStorage.setItem(\"user-authed\", JSON.stringify(false));\r\n      firebase.doSignOut();\r\n      window.location.reload();\r\n    }\r\n\r\n    //sing in\r\n    else {\r\n      setLoginOpen(true);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"nav-bar\">\r\n      <div className=\"nav-bar__left-part\">\r\n        <Link className=\"nav-bar__links\" to=\"/men\">\r\n          Men\r\n        </Link>\r\n        <Link className=\"nav-bar__links\" to=\"/women\">\r\n          Women\r\n        </Link>\r\n        <Link className=\"nav-bar__links\" to=\"/kids\">\r\n          Kids\r\n        </Link>\r\n      </div>\r\n\r\n      <a href=\"/home\">\r\n        <img className=\"nav-bar__logo\" src={Logo} alt=\"logo\" />\r\n      </a>\r\n\r\n      <div className=\"nav-bar__right-part\">\r\n        <a className=\"nav-bar__links\" onClick={() => setSearchOpen(true)}>\r\n          Search\r\n        </a>\r\n        <a className=\"nav-bar__links\" onClick={SingOut_SingIn} id=\"singIn\">\r\n          {userAuthed ? \"Sing Out\" : \"Sing In\"}\r\n        </a>\r\n        <Link className=\"nav-bar__links\" to=\"/bag\">\r\n          {`My Card (${cardLength})`}\r\n        </Link>\r\n        {userAuthed && (\r\n          <Link className=\"nav-bar__links\" to=\"/profile\">\r\n            My Profile\r\n          </Link>\r\n        )}\r\n      </div>\r\n\r\n      {/* Down here is the mobile version of this nav bar */}\r\n      <MobileNavBar\r\n        setMenuOpen={setMenuOpen}\r\n        menuOpen={menuOpen}\r\n        setSearchOpen={setSearchOpen}\r\n        SingOut_SingIn={SingOut_SingIn}\r\n        userAuthed={userAuthed}\r\n        cardLength={cardLength}\r\n      />\r\n\r\n      {/* down here is the condition rendering of search and authentification components */}\r\n      <AnimatePresence>\r\n        {searchOpen && <Search closeSearch={() => setSearchOpen(false)} />}\r\n\r\n        {loginOpen && (\r\n          <Singin\r\n            closeLogin={() => setLoginOpen(false)}\r\n            openSingUp={() => setSingupOpen(true)}\r\n            openPasswordReset={() => setPasswordReset(true)}\r\n            firebase={firebase}\r\n          />\r\n        )}\r\n\r\n        {singupOpen && (\r\n          <SingUp\r\n            closeSingup={() => setSingupOpen(false)}\r\n            firebase={firebase}\r\n          />\r\n        )}\r\n\r\n        {passwordResetOpen && (\r\n          <PasswordReset\r\n            closePasswordReset={() => setPasswordReset(false)}\r\n            firebase={firebase}\r\n          />\r\n        )}\r\n      </AnimatePresence>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default NavBar;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\firebase-config.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singin\\singin.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\search\\search.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\mobileNavBar\\mobileNavBar.jsx",["329","330"],"import R