const SingOut_SingIn = () => {\r\n    //sing out\r\n    if (userAuthed) {\r\n      localStorage.setItem(\"user-authed\", JSON.stringify(false));\r\n      firebase.doSignOut();\r\n      window.location.reload();\r\n    }\r\n\r\n    //sing in\r\n    else {\r\n      setLoginOpen(true);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"nav-bar\">\r\n      <div className=\"nav-bar__left-part\">\r\n        <Link className=\"nav-bar__links\" to=\"/men\">\r\n          Men\r\n        </Link>\r\n        <Link className=\"nav-bar__links\" to=\"/women\">\r\n          Women\r\n        </Link>\r\n        <Link className=\"nav-bar__links\" to=\"/kids\">\r\n          Kids\r\n        </Link>\r\n      </div>\r\n\r\n      <a href=\"/home\">\r\n        <img className=\"nav-bar__logo\" src={Logo} alt=\"logo\" />\r\n      </a>\r\n\r\n      <div className=\"nav-bar__right-part\">\r\n        <a className=\"nav-bar__links\" onClick={() => setSearchOpen(true)}>\r\n          Search\r\n        </a>\r\n        <a className=\"nav-bar__links\" onClick={SingOut_SingIn} id=\"singIn\">\r\n          {userAuthed ? \"Sing Out\" : \"Sing In\"}\r\n        </a>\r\n        <Link className=\"nav-bar__links\" to=\"/bag\">\r\n          {`My Card (${cardLength})`}\r\n        </Link>\r\n        {userAuthed && (\r\n          <Link className=\"nav-bar__links\" to=\"/profile\">\r\n            My Profile\r\n          </Link>\r\n        )}\r\n      </div>\r\n\r\n      {/* Down here is the mobile version of this nav bar */}\r\n      <MobileNavBar\r\n        setMenuOpen={setMenuOpen}\r\n        menuOpen={menuOpen}\r\n        setSearchOpen={setSearchOpen}\r\n        SingOut_SingIn={SingOut_SingIn}\r\n        userAuthed={userAuthed}\r\n        cardLength={cardLength}\r\n      />\r\n\r\n      {/* down here is the condition rendering of search and authentification components */}\r\n      <AnimatePresence>\r\n        {searchOpen && <Search closeSearch={() => setSearchOpen(false)} />}\r\n\r\n        {loginOpen && (\r\n          <Singin\r\n            closeLogin={() => setLoginOpen(false)}\r\n            openSingUp={() => setSingupOpen(true)}\r\n            openPasswordReset={() => setPasswordReset(true)}\r\n            firebase={firebase}\r\n          />\r\n        )}\r\n\r\n        {singupOpen && (\r\n          <SingUp\r\n            closeSingup={() => setSingupOpen(false)}\r\n            firebase={firebase}\r\n          />\r\n        )}\r\n\r\n        {passwordResetOpen && (\r\n          <PasswordReset\r\n            closePasswordReset={() => setPasswordReset(false)}\r\n            firebase={firebase}\r\n          />\r\n        )}\r\n      </AnimatePresence>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default NavBar;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\firebase-config.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singin\\singin.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\search\\search.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\mobileNavBar\\mobileNavBar.jsx",["312","313"],"import React, { Fragment } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nimport { ReactComponent as Close } from \"../../../../assets/img/close.svg\";\r\nimport { ReactComponent as Menu } from \"../../../../assets/img/menu.svg\";\r\n\r\nconst MobileNavBar = ({\r\n  setMenuOpen,\r\n  menuOpen,\r\n  setSearchOpen,\r\n  SingOut_SingIn,\r\n  userAuthed,\r\n  cardLength,\r\n}) => {\r\n  const closeNavBarSlider = ({ target }) => {\r\n    if (target.className !== \"nav-bar__mobile\") setMenuOpen(false);\r\n  };\r\n\r\n  return (\r\n    <Fragment>\r\n      <Menu\r\n        className=\"nav-bar__menu-bar\"\r\n        stroke=\"black\"\r\n        onClick={() => setMenuOpen(true)}\r\n      />\r\n\r\n      <motion.div\r\n        animate={!menuOpen ? { left: \"100%\" } : { left: 0 }}\r\n        transition={{ duration: 0.4 }}\r\n        className=\"nav-bar__mobile\"\r\n        onClick={closeNavBarSlider}\r\n      >\r\n        <Close className=\"nav-bar__close\" fill=\"white\" />\r\n\r\n        <Link className=\"nav-bar__links--white\" to=\"/men\">\r\n          Men\r\n        </Link>\r\n        <Link className=\"nav-bar__links--white\" to=\"/women\">\r\n          Women\r\n        </Link>\r\n        <Link className=\"nav-bar__links--white\" to=\"/kids\">\r\n          Kids\r\n        </Link>\r\n        <a\r\n          className=\"nav-bar__links--white\"\r\n          onClick={() => setSearchOpen(true)}\r\n        >\r\n          Search\r\n        </a>\r\n        <a className=\"nav-bar__links--white\" onClick={SingOut_SingIn}>\r\n          {userAuthed ? \"Sing Out\" : \"Sing In\"}\r\n        </a>\r\n        <Link className=\"nav-bar__links--white\" to=\"/bag\">\r\n          {`My Card (${cardLength})`}\r\n        </Link>\r\n        {userAuthed && (\r\n          <Link className=\"nav-bar__links\" to=\"/profile\">\r\n            My Profile\r\n          </Link>\r\n        )}\r\n      </motion.div>\r\n    </Fragment>\r\n  );\r\n};\r\n\r\nexport default MobileNavBar;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singin\\passwordReset\\passwordReset.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singup\\singup.jsx",["314"],"import React, { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport InputsWrapper from \"./../../inputsWrapper/inputsWrapper\";\r\n\r\nimport { handleErrors } from \"../../../../server/firebase/errorHandling\";\r\nimport {\r\n  checkErrors,\r\n  useCustomFormik,\r\n  closeFormWithNoBubbling,\r\n} from \"../helper-functions\";\r\nimport { generateSingupInputs } from \"../inputs-list\";\r\nimport \"./singup.scss\";\r\nimport { ReactComponent as Close } from \"../../../../assets/img/close.svg\";\r\nimport Logo from \"../../../../assets/img/logo.png\";\r\n\r\nconst SingUp = ({ closeSingup, firebase }) => {\r\n  const [registering, setRegistering] = useState(false);\r\n  const [globalErrors, setGlobalErrors] = useState(null);\r\n\r\n  //Formik init\r\n  const {\r\n    handleSubmit,\r\n    touched,\r\n    errors,\r\n    handleChange,\r\n    values,\r\n    handleBlur,\r\n    setErrors,\r\n  } = useCustomFormik(\"singup\", doSubmit);\r\n\r\n  async function doSubmit({ email, password, fullName }) {\r\n    setGlobalErrors(null);\r\n    try {\r\n      setRegistering(true);\r\n      const data = await firebase.doCreateUserWithEmailAndPassword(\r\n        email,\r\n        password\r\n      );\r\n      //store the json web token in the localstorage\r\n      localStorage.setItem(\r\n        \"user-authed\",\r\n        JSON.stringify(\r\n          data.user.ya.split(\".\")[0] + \".\" + data.user.ya.split(\".\")[1]\r\n        )\r\n      );\r\n      //add the new user to database\r\n      firebase.addUser({\r\n        uid: data.user.uid,\r\n        name: fullName,\r\n        email,\r\n      });\r\n      closeSingup();\r\n      window.location.reload();\r\n    } catch (error) {\r\n      handleErrors(\"singup\", error, setErrors, setGlobalErrors);\r\n    }\r\n    setRegistering(false);\r\n  }\r\n\r\n  return (\r\n    <motion.div\r\n      transition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      className=\"singup--background\"\r\n      onClick={e => closeFormWithNoBubbling(e, closeSingup)}\r\n      onKeyPress={e => {\r\n        if (e.key === \"Enter\") handleSubmit();\r\n      }}\r\n    >\r\n      <motion.div\r\n        className=\"singup\"\r\n        initial={{ opacity: 0 }}\r\n   