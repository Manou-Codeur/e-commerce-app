/a>\r\n        <a className=\"nav-bar__links\" onClick={SingOut_SingIn}>\r\n          {userAuthed ? \"Sing Out\" : \"Sing In\"}\r\n        </a>\r\n        <Link className=\"nav-bar__links\" to=\"/bag\">\r\n          MyCart(1)\r\n        </Link>\r\n      </div>\r\n\r\n      {/* Down here is the mobile version of this nav bar */}\r\n      <MobileNavBar\r\n        setMenuOpen={setMenuOpen}\r\n        menuOpen={menuOpen}\r\n        setSearchOpen={setSearchOpen}\r\n        SingOut_SingIn={SingOut_SingIn}\r\n        userAuthed={userAuthed}\r\n      />\r\n\r\n      {/* down here is the condition rendering of search and authentification components */}\r\n      <AnimatePresence>\r\n        {searchOpen ? (\r\n          <Search closeSearch={() => setSearchOpen(false)} />\r\n        ) : null}\r\n\r\n        {loginOpen ? (\r\n          <Singin\r\n            closeLogin={() => setLoginOpen(false)}\r\n            openSingUp={() => setSingupOpen(true)}\r\n            openPasswordReset={() => setPasswordReset(true)}\r\n            firebase={firebase}\r\n          />\r\n        ) : null}\r\n\r\n        {singupOpen ? (\r\n          <SingUp\r\n            closeSingup={() => setSingupOpen(false)}\r\n            firebase={firebase}\r\n          />\r\n        ) : null}\r\n\r\n        {passwordResetOpen ? (\r\n          <PasswordReset\r\n            closePasswordReset={() => setPasswordReset(false)}\r\n            firebase={firebase}\r\n          />\r\n        ) : null}\r\n      </AnimatePresence>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default NavBar;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\firebase-config.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singin\\singin.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\sea