-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\profile.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\orders\\orders.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\account\\account.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\account\\inputsList.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\account\\yupSchema.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\context\\orderContext.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\orders\\ordersWrapper\\ordersWrapper.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\profile\\orders\\ordersWrapper\\order\\order.jsx",["343"],"import React from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport \"./order.scss\";\r\n\r\nconst Order = ({ data }) => {\r\n  const generatePrice = () => {\r\n    const price = data.price;\r\n\r\n    if (data.amount > 1) {\r\n      return `(${data.amount}) * ${data.price}.00`;\r\n    }\r\n    return `${data.price}.00`;\r\n  };\r\n\r\n  return (\r\n    <motion.div\r\n      initial={{ opacity: 0, y: 10 }}\r\n      animate={{ opacity: 1, y: 0 }}\r\n      transition={{ duration: 0.3 }}\r\n      className=\"order\"\r\n    >\r\n      <div className=\"order__img\">\r\n        <img src={data.img} alt=\"\" />\r\n      </div>\r\n\r\n      <div className=\"order__info\">\r\n        <div className=\"order__date\">May 31,2020</div>\r\n        <div className=\"order__state\">\r\n          <span>Shipped</span>\r\n          <span>{generatePrice()}</span>\r\n        </div>\r\n      </div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default Order;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\product-page\\review-comp\\reviews-area\\reviews-area.jsx",["344"],"import React, { useEffect, useState } from \"react\";\r\n\r\nimport ReviewsWrapper from \"./reviews-wrapper/reviews-wrapper\";\r\n\r\nimport \"./reviews-area.scss\";\r\n\r\nconst ReviewsArea = ({ productId, firebase }) => {\r\n  const [reviews, setReviews] = useState(null);\r\n  const [errors, setErrors] = useState(false);\r\n\r\n  useEffect(() => {\r\n    fetchReviews();\r\n\r\n    //cleanup firebase subscription once the component get unmounted\r\n    return () => {\r\n      firebase.getProductReviews(productId).off();\r\n    };\r\n  }, [productId]);\r\n\r\n  useEffect(() => {\r\n    //handle unexpected errors; if the user is still seeying the \"loading\" msg for 10 sec\r\n    const timer = setTimeout(() => {\r\n      if (!reviews) {\r\n        setErrors(true);\r\n      }\r\n    }, 10000);\r\n\r\n    return () => {\r\n      clearTimeout(timer);\r\n    };\r\n  });\r\n\r\n  const fetchReviews = () => {\r\n    const data = firebase.getProductReviews(productId);\r\n    data.on(\"value\", snapshot => {\r\n      const reviewsObj = snapshot.val();\r\n      if (reviewsObj) setReviews(Object.values(reviewsObj));\r\n      else setReviews([]);\r\n    });\r\n  };\r\n\r\n  return (\r\n    <div className=\"reviews-area\">\r\n      <h1>Reviews</h1>\r\n\r\n      <ReviewsWrapper reviews={reviews} errors={errors} />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ReviewsArea;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\product-page\\review-comp\\reviews-area\\reviews-wrapper\\reviews-wrapper.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\