\r\n    else setCardHolder(values.holderName);\r\n  }, [values.holderName]);\r\n\r\n  useEffect(() => {\r\n    if (values.expirationDate === \"\") setCardExp(\"MM/YY\");\r\n    else setCardExp(`${values.expirationDate}/${values.year}`);\r\n  }, [values.expirationDate, values.year]);\r\n\r\n  const customHandleChange = e => {\r\n    const { value, name } = e.target;\r\n    const numRegx = /^\\d+$/;\r\n    const nameRegx = /^[a-zA-Z ]+$/;\r\n\r\n    if (name === \"cardNumber\") {\r\n      if ((value !== \"\" && !numRegx.test(value)) || value.length > 16) return;\r\n    }\r\n\r\n    if (name === \"holderName\") {\r\n      if (value !== \"\" && !nameRegx.test(value)) return;\r\n    }\r\n\r\n    if (name === \"cvv\") {\r\n      if ((value !== \"\" && !numRegx.test(value)) || value.length > 4) return;\r\n    }\r\n\r\n    handleChange(e);\r\n  };\r\n\r\n  const doSubmit = values => {\r\n    console.log(\"submit!!\");\r\n  };\r\n\r\n  return (\r\n    <div className=\"form\">\r\n      <div className=\"form__line\">\r\n        <InputsWrapper\r\n          inputs={generateLine1And2(values, errors, touched)}\r\n          eventsFunctions={{ onChange: customHandleChange, onBlur: handleBlur }}\r\n        />\r\n      </div>\r\n\r\n      <div className=\"form__line\">\r\n        <InputsWrapper\r\n          inputs={generateLine3(values, errors, touched)}\r\n          eventsFunctions={{ onChange: customHandleChange, onBlur: handleBlur }}\r\n        />\r\n      </div>\r\n\r\n      <button className=\"form__submit\" onClick={handleSubmit}>\r\n        Submit\r\n      </button>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Form;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\card\\card.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\form\\inputsList.js",[],"C:\\Users\