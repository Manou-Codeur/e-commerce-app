sition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      className=\"singup--background\"\r\n      onClick={e => closeFormWithNoBubbling(e, closeSingup)}\r\n      onKeyPress={e => {\r\n        if (e.key === \"Enter\") handleSubmit();\r\n      }}\r\n    >\r\n      <motion.div\r\n        className=\"singup\"\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        exit={{ y: 5 }}\r\n        transition={{ duration: 0.5 }}\r\n      >\r\n        <Close className=\"singup__close-icon\" onClick={closeSingup} />\r\n\r\n        <img src={Logo} className=\"singup__logo\" alt=\"logo\" />\r\n\r\n        <h1>Become a member</h1>\r\n\r\n        {globalErrors && (\r\n          <div style={{ color: \"red\", transform: \"translateY(170%)\" }}>\r\n            {globalErrors}\r\n          </div>\r\n        )}\r\n\r\n        <InputsWrapper\r\n          inputs={generateSingupInputs(values, errors, touched)}\r\n          eventsFunctions={{ onChange: handleChange, onBlur: handleBlur }}\r\n        />\r\n\r\n        <button\r\n          className=\"singup__btn singup__btn--margin\"\r\n          disabled={registering || Object.keys(errors).length > 0}\r\n          onClick={handleSubmit}\r\n        >\r\n          {registering ? \"REGISTERING\" : \"REGISTER\"}\r\n        </button>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default SingUp;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\fake-db\\db-functions.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\errorHandling.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\inputs-list.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\helper-functions.js",[],"C:\\