ord-reset--background\"\r\n      transition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      onClick={e => closeFormWithNoBubbling(e, closePasswordReset)}\r\n    >\r\n      <motion.div\r\n        className=\"password-reset\"\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        transition={{ duration: 0.5 }}\r\n        exit={{ y: 5 }}\r\n      >\r\n        <Close\r\n          className=\"password-reset__close-icon\"\r\n          onClick={closePasswordReset}\r\n        />\r\n\r\n        <img src={Logo} className=\"password-reset__logo\" />\r\n\r\n        <h1>Reset your password</h1>\r\n\r\n        {globalErrors && (\r\n          <div style={{ color: \"red\", transform: \"translateY(170%)\" }}>\r\n            {globalErrors}\r\n          </div>\r\n        )}\r\n\r\n        <InputsWrapper\r\n          inputs={generateResetPasswordInputs(values, errors, touched)}\r\n          eventsFunctions={{\r\n            onChange: handleChange,\r\n            onBlur: handleBlur,\r\n          }}\r\n        />\r\n\r\n        <button\r\n          className=\"password-reset__btn password-reset__btn--margin\"\r\n          type=\"submit\"\r\n          onClick={handleSubmit}\r\n          disabled={resetting || errors.email}\r\n        >\r\n          {resetting ? \"RESETTING...\" : \"RESET\"}\r\n        </button>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default PasswordReset;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singup\\singup.jsx",["277"],"import React, { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport InputsWrapper from \"./../../inputsWrapper/inputsWrapper\";\r\n\r\nimport { handleErrors } from \"../../../../server/firebase/errorHandling\";\r\nimport {\r\n  checkErrors,\r\n  useCustomFormik,\r\n  closeFormWithNoBubbling,\r\n} from \"../helper-functions\";\r\nimport { generateSingupInputs } from \"../inputs-list\";\r\nimport \"./singup.scss\";\r\nimport { ReactComponent as Close } from \"../../../../assets/img/close.svg\";\r\nimport Logo from \"../../../../assets/img/logo.png\";\r\n\r\nconst SingUp = ({ closeSingup, firebase }) => {\r\n  const [registering, setRegistering] = useState(false);\r\n  const [globalErrors, setGlobalErrors] = useState(null);\r\n\r\n  //Formik init\r\n  const {\r\n    handleSubmit,\r\n    touched,\r\n    errors,\r\n    handleChange,\r\n    values,\r\n    handleBlur,\r\n    setErrors,\r\n  } = useCustomFormik(\"singup\", doSubmit);\r\n\r\n  async function doSubmit({ email, password }) {\r\n    setGlobalErrors(null);\r\n    try {\r\n      setRegistering(true);\r\n      const data = await firebase.doCreateUserWithEmailAndPassword(\r\n        email,\r\n        password\r\n      );\r\n      //store the json web token in the localstorage\r\n      localStorage.setItem(\r\n        \"user-authed\",\r\n        JSON.stringify(\r\n          data.user.ya.split(\".\")[0] + \".\" + data.user.ya.split(\".\")[1]\r\n        )\r\n      );\r\n      closeSingup();\r\n    } catch (error) {\r\n      handleErrors(\"singup\", error, setErrors, setGlobalErrors);\r\n    }\r\n    setRegistering(false);\r\n  }\r\n\r\n  return (\r\n    <motion.div\r\n      transition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      className=\"singup--background\"\r\n      onClick={e => closeFormWithNoBubbling(e, closeSingup)}\r\n      onKeyPress={e => {\r\n        if (e.key === \"Enter\") handleSubmit();\r\n      }}\r\n    >\r\n      <motion.div\r\n        className=\"singup\"\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        exit={{ y: 5 }}\r\n        transition={{ duration: 0.5 }}\r\n      >\r\n        <Close className=\"singup__close-icon\" onClick={closeSingup} />\r\n\r\n        <img src={Logo} className=\"singup__logo\" />\r\n\r\n        <h1>Become a member</h1>\r\n\r\n        {globalErrors && (\r\n          <div style={{ color: \"red\", transform: \"translateY(170%)\" }}>\r\n            {globalErrors}\r\n          </div>\r\n        )}\r\n\r\n        <InputsWrapper\r\n          inputs={generateSingupInputs(values, errors, touched)}\r\n          eventsFunctions={{ onChange: handleChange, onBlur: handleBlur }}\r\n        />\r\n\r\n        <button\r\n          className=\"singup__btn singup__btn--margin\"\r\n          disabled={registering || checkErrors(errors)}\r\n          onClick={handleSubmit}\r\n        >\r\n          {registering ? \"REGISTERING\" : \"REGISTER\"}\r\n        </button>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default SingUp;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\fake-db\\db-functions.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\errorHandling.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\inputs-list.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\helper-functions.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\inputsWrapper\\inputsWrapper.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\search\\search-input\\search-input.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\search\\search-result\\search-result.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\yup-schema.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\j