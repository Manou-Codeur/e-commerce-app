e: \"\",\r\n      expirationDate: \"\",\r\n      year: \"\",\r\n      cvv: \"\",\r\n    },\r\n    validationSchema: Yup.object(checkoutInputsSchema),\r\n    onSubmit: values => {\r\n      doSubmit(values);\r\n    },\r\n  });\r\n\r\n  const CARDS = {\r\n    visa: \"^4\",\r\n    amex: \"^(34|37)\",\r\n    mastercard: \"^5[1-5]\",\r\n    discover: \"^6011\",\r\n    unionpay: \"^62\",\r\n    troy: \"^9792\",\r\n    diners: \"^(30[0-5]|36)\",\r\n  };\r\n\r\n  const generateCardType = cardNumber => {\r\n    const number = cardNumber;\r\n    let re;\r\n    for (const [card, pattern] of Object.entries(CARDS)) {\r\n      re = new RegExp(pattern);\r\n      if (number.match(re) != null) {\r\n        return card;\r\n      }\r\n    }\r\n\r\n    return \"visa\"; // default type\r\n  };\r\n\r\n  const formalizeCardNumber = number => {\r\n    let final = \"\";\r\n    for (let i = 0; i < number.length; i++) {\r\n      if (i % 4 === 0) final += \" \";\r\n      final += number[i];\r\n    }\r\n    return final;\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (values.cardNumber === \"\") {\r\n      setCardNumber(\"XXXX XXXX XXXX XXXX\");\r\n      setCardType(\"visa\");\r\n    } else {\r\n      setCardNumber(formalizeCardNumber(values.cardNumber));\r\n      setCardType(generateCardType(values.cardNumber));\r\n    }\r\n  }, [values.cardNumber]);\r\n\r\n  useEffect(() => {\r\n    if (values.holderName === \"\") setCardHolder(\"UserName\");\r\n    else setCardHolder(values.holderName);\r\n  }, [values.holderName]);\r\n\r\n  useEffect(() => {\r\n    if (values.expirationDate === \"\") setCardExp(\"MM/YY\");\r\n    else setCardExp(`${values.expirationDate}/${values.year}`);\r\n  }, [values.expirationDate, values.year]);\r\n\r\n  const customHandleChange = e => {\r\n    const { value, name } = e.target;\r\n    const numRegx = /^\\d+$/;\r\n    const nameRegx = /^[a-zA-Z ]+$/;\r\n\r\n    if (name === \"cardNumber\") {\r\n      if ((value !== \"\" && !numRegx.test(value)) || value.length > 16) return;\r\n    }\r\n\r\n    if (name === \"holderName\") {\r\n      if (value !== \"\" && !nameRegx.test(value)) return;\r\n    }\r\n\r\n    if (name === \"cvv\") {\r\n      if ((value !== \"\" && !numRegx.test(value)) || value.length > 4) return;\r\n    }\r\n\r\n    handleChange(e);\r\n  };\r\n\r\n  const doSubmit = values => {\r\n    console.log(\"submit!!\");\r\n  };\r\n\r\n  return (\r\n    <div className=\"form\">\r\n      <div className=\"form__line\">\r\n        <InputsWrapper\r\n          inputs={generateLine1And2(values, errors, touched)}\r\n          eventsFunctions={{ onChange: customHandleChange, onBlur: handleBlur }}\r\n        />\r\n      </div>\r\n\r\n      <div className=\"form__line\">\r\n        <InputsWrapper\r\n          inputs={generateLine3(values, errors, touched)}\r\n          eventsFunctions={{ onChange: customHandleChange, onBlur: handleBlur }}\r\n        />\r\n      </div>\r\n\r\n      <button className=\"form__submit\" onClick={handleSubmit}>\r\n        Submit\r\n      </button>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Form;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\card\\card.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\form\\inputsList.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\form\\yupShema.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\firebaseContext.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\store\\configureStore.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\store\\card.js",["296"