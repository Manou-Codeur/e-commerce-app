p\\src\\components\\main\\product-page\\product-page.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\genreProduct\\genreProduct.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\order.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\notFound\\notFound.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\context\\historyContext.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\home\\header\\header.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\navBar.jsx",["220","221","222"],"import React, { useState } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { AnimatePresence } from \"framer-motion\";\r\nimport jwtGenerator from \"jwt-decode\";\r\n\r\nimport Search from \"./search/search\";\r\nimport Singin from \"./singin/singin\";\r\nimport SingUp from \"./singup/singup\";\r\nimport MobileNavBar from \"./mobileNavBar/mobileNavBar\";\r\nimport PasswordReset from \"./singin/passwordReset/passwordReset\";\r\nimport Firebase from \"./../../../server/firebase/firebase-config\";\r\n\r\nimport \"./navBar.scss\";\r\nimport Logo from \"../../../assets/img/logo.png\";\r\n\r\nconst firebase = new Firebase();\r\n\r\nconst NavBar = () => {\r\n  const [searchOpen, setSearchOpen] = useState(false);\r\n  const [menuOpen, setMenuOpen] = useState(false);\r\n  const [loginOpen, setLoginOpen] = useState(false);\r\n  const [singupOpen, setSingupOpen] = useState(false);\r\n  const [passwordResetOpen, setPasswordReset] = useState(false);\r\n  //this state is used to force rerendering login/logout\r\n  const [render, setRender] = useState(false);\r\n\r\n  //I wrapper this code in trycatch coz the jwtGenerator() throw an error if the string passed isn't valid jwt code\r\n  try {\r\n    const jwt = jwtGenerator(JSON.parse(localStorage.getItem(\"user-authed\")));\r\n    var userAuthed = jwt.aud === \"react-e-commerce-app-18fea\";\r\n  } catch (error) {}\r\n\r\n  const SingOut_SingIn = () => {\r\n    //sing out\r\n    if (userAuthed) {\r\n      localStorage.setItem(\"user-authed\", JSON.stringify(false));\r\n      firebase.doSignOut();\r\n      setRender(prev => !prev);\r\n    }\r\n\r\n    //sing in\r\n    else {\r\n      setLoginOpen(true);\r\n      setRender(prev => !prev);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"nav-bar\">\r\n      <div className=\"nav-bar__left-part\">\r\n        <Link className=\"nav-bar__links\" to=\"/men\">\r\n          Men\r\n        </Link>\r\n        <Link className=\"nav-bar__links\" to=\"/women\">\r\n          Women\r\n        </Link>\r\n        <Link className=\"nav-bar__links\" to=\"/kid\">\r\n          Kids\r\n        </Link>\r\n      </div>\r\n\r\n      <a href=\"/home\">\r\n        <img className=\"nav-bar__logo\" src={Logo} alt=\"logo\" />\r\n      </a>\r\n\r\n      <div className=\"nav-bar__right-part\">\r\n        <a className=\"nav-bar__links\" onClick={() => setSearchOpen(true)}>\r\n          Search\r\n        </a>\r\n        <a className=\"nav-bar__links\" onClick={SingOut_SingIn}>\r\n          {userAuthed ? \"Sing Out\" : \"Sing In\"}\r\n        </a>\r\n        <Link className=\"nav-bar__links\" to=\"/bag\">\r\n          MyCart(1)\r\n        </Link>\r\n      </div>\r\n\r\n      {/* Down here is the mobile version of this nav bar */}\r\n      <MobileNavBar\r\n        setMenuOpen={setMenuOpen}\r\n        menuOpen={menuOpen}\r\n        setSearchOpen={setSearchOpen}\r\n        SingOut_SingIn={SingOut_SingIn}\r\n        userAuthed={userAuthed}\r\n      />\r\n\r\n      {/* down here is the condition rendering of search and authentification components */}\r\n      <AnimatePresence>\r\n        {searchOpen ? (\r\n          <Search closeSearch={() => setSearchOpen(false)} />\r\n        ) : null}\r\n\r\n        {loginOpen ? (\r\n          <Singin\r\n            closeLogin={() => setLoginOpen(false)}\r\n            openSingUp={() => setSingupOpen(true)}\r\n            openPasswordReset={() => setPasswordReset(true)}\r\n            firebase={firebase}\r\n          />\r\n        ) : null}\r\n\r\n        {singupOpen ? (\r\n          <SingUp\r\n            closeSingup={() => setSingupOpen(false)}\r\n            firebase={firebase}\r\n          />\r\n        ) : null}\r\n\r\n        {passwordResetOpen ? (\r\n          <PasswordReset\r\n            closePasswordReset={() => setPasswordReset(false)}\r\n            firebase={firebase}\r\n          />\r\n        ) : null}\r\n      </AnimatePresence>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default NavBar;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\firebase-config.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singin\\singin.jsx",["223"],"import React, { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport InputsWrapper from \"../../inputsWrapper/inputsWrapper\";\r\n\r\nimport { handleErrors } from \"../../../../server/firebase/errorHandling\";\r\nimport {\r\n  checkErrors,\r\n  useCustomFormik,\r\n  closeFormWithNoBubbling,\r\n} from \"../helper-functions\";\r\nimport { generateSinginInputs } from \"../inputs-list\";\r\nimport \"./singin.scss\";\r\nimport { ReactComponent as Close } from \"../../../../assets/img/close.svg\";\r\nimport Logo from \"../../../../assets/img/logo.png\";\r\n\r\nconst Singin = ({ closeLogin, openSingUp, firebase, openPasswordReset }) => {\r\n  const [logging, setLogging] = useState(false);\r\n  const [globalErrors, setGlobalErrors] = useState(null);\r\n\r\n  //Formik init\r\n  const {\r\n    handleSubmit,\r\n    touched,\r\n    errors,\r\n    handleChange,\r\n    values,\r\n    handleBlur,\r\n    setErrors,\r\n  } = useCustomFormik(\"singin\", doSubmit);\r\n\r\n  async function doSubmit({ email, password }) {\r\n    setGlobalErrors(null);\r\n    try {\r\n      setLogging(true);\r\n      const data = await firebase.doSignInWithEmailAndPassword(email, password);\r\n      //store the json web token in the localstorage\r\n      localStorage.setItem(\r\n        \"user-authed\",\r\n        JSON.stringify(\r\n          data.user.ya.split(\".\")[0] + \".\" + data.user.ya.split(\".\")[1]\r\n        )\r\n      );\r\n      closeLogin();\r\n    } catch (error) {\r\n      handleErrors(\"singin\", error, setErrors, setGlobalErrors);\r\n    }\r\n    setLogging(false);\r\n  }\r\n\r\n  return (\r\n    <motion.div\r\n      className=\"login--background\"\r\n      initial={{ opacity: 0 }}\r\n      animate={{ opacity: 1 }}\r\n      transition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      onClick={e => closeFormWithNoBubbling(e, closeLogin)}\r\n      onKeyPress={e => {\r\n        if (e.key === \"Enter\") handleSubmit();\r\n      }}\r\n    >\r\n      <motion.div\r\n        className=\"login\"\r\n        initial={{ y: 15 }}\r\n        animate={{ y: 0 }}\r\n        transition={{ duration: 0.5 }}\r\n        exit={{ y: 15 }}\r\n      >\r\n        <Close className=\"login__close-icon\" onClick={closeLogin} />\r\n\r\n        <img src={Logo} className=\"login__logo\" />\r\n\r\n        <h1>Log in your account</h1>\r\n\r\n        {globalErrors && (\r\n          <div style={{ color: \"red\", transform: \"translateY(170%)\" }}>\r\n            {globalErrors}\r\n          </div>\r\n        )}\r\n\r\n        <InputsWrapper\r\n          inputs={generateSinginInputs(values, errors, touched)}\r\n          eventsFunctions={{ onChange: handleChange, onBlur: handleBlur }}\r\n        />\r\n\r\n        <span\r\n          className=\"login__forgetPassword\"\r\n          onClick={() => {\r\n            closeLogin();\r\n            openPasswordReset();\r\n          }}\r\n        >\r\n          Forget password ?\r\n        </span>\r\n\r\n        <button\r\n          className=\"login__btn\"\r\n          type=\"submit\"\r\n          onClick={handleSubmit}\r\n          disabled={logging || checkErrors(errors)}\r\n        >\r\n          {logging ? \"LOGGING...\" : \"LOGIN\"}\r\n        </button>\r\n\r\n        <p className=\"login__goToRegister\">\r\n          You are not member yet?\r\n          <span\r\n            onClick={() => {\r\n              closeLogin();\r\n              openSingUp();\r\n            }}\r\n          >\r\n            Register now.\r\n          </span>\r\n        </p>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default Singin;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\search\\search.jsx",["224"],"import React, { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport SearchInput from \"./search-input/search-input\";\r\nimport SearchResult from \"./search-result/search-result\";\r\n\r\nimport { filterProducts } from \"../../../../server/fake-db/db-functions\";\r\nimport \"./search.scss\";\r\nimport Logo from \"../../../../assets/img/logo.png\";\r\nimport { ReactComponent as Close } from \"../../../../assets/img/close.svg\";\r\n\r\nconst Search = ({ closeSearch }) => {\r\n  const [filtredProducts, setFiltredProducts] = useState([]);\r\n\r\n  const onQueryChange = ({ target }) => {\r\n    setFiltredProducts(filterProducts(target.value));\r\n  };\r\n\r\n  return (\r\n    <motion.div\r\n      className=\"search--background\"\r\n      initial={{ opacity: 0 }}\r\n      animate={{ opacity: 1 }}\r\n      transition={{ duration: 1.2 }}\r\n      exit={{ opacity: 0 }}\r\n    >\r\n      <motion.div\r\n        className=\"search\"\r\n        initial={{ y: \"-100%\" }}\r\n        animate={{ y: \"0%\" }}\r\n        transition={{ duration: 0.8 }}\r\n        exit={{ y: \"-100%\" }}\r\n      >\r\n        <div className=\"search__up-part\">\r\n          <img src={Logo} className=\"search__logo\" />\r\n\r\n          <SearchInput onchange={onQueryChange} />\r\n\r\n          <div className=\"search__close-containner\">\r\n            <Close\r\n              fill=\"black\"\r\n              className=\"search__close-icon\"\r\n              onClick={closeSearch}\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"search__down-part\">\r\n          <SearchResult\r\n            filtredProducts={filtredProducts}\r\n            closeSearch={closeSearch}\r\n          />\r\n        </div>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default Search;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\mobileNavBar\\mobileNavBar.jsx",["225","226"],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singin\\passwordReset\\passwordReset.jsx",["227"],"import React, { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport InputsWrapper from \"../../../inputsWrapper/inputsWrapper\";\r\n\r\nimport {\r\n  useCustomFormik,\r\n  closeFormWithNoBubbling,\r\n} from \"./../../helper-functions\";\r\nimport { handleErrors } from \"./../../../../../server/firebase/errorHandling\";\r\nimport { generateResetPasswordInputs } from \"../../inputs-list\";\r\nimport \"./passwordReset.scss\";\r\nimport { ReactComponent as Close } from \"../../../../../assets/img/close.svg\";\r\nimport Logo from \"../../../../../assets/img/logo.png\";\r\n\r\nconst PasswordReset = ({ closePasswordReset, firebase }) => {\r\n  const [resetting, setResetting] = useState(false);\r\n  const [globalErrors, setGlobalErrors] = useState(null);\r\n\r\n  //Formik init\r\n  const {\r\n    handleSubmit,\r\n    touched,\r\n    errors,\r\n    handleChange,\r\n    values,\r\n    handleBlur,\r\n    setErrors,\r\n  } = useCustomFormik(\"resetPassword\", doResetPassword);\r\n\r\n  async function doResetPassword(email) {\r\n    setGlobalErrors(null);\r\n    try {\r\n      setResetting(true);\r\n      await firebase.doResetPassword(email);\r\n      closePasswordReset();\r\n    } catch (error) {\r\n      handleErrors(\"resetPassword\", error, setErrors, setGlobalErrors);\r\n    }\r\n    setResetting(false);\r\n  }\r\n\r\n  return (\r\n    <motion.div\r\n      className=\"password-reset--background\"\r\n      transition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      onClick={e => closeFormWithNoBubbling(e, closePasswordReset)}\r\n    >\r\n      <motion.div\r\n        className=\"password-reset\"\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        transition={{ duration: 0.5 }}\r\n        exit={{ y: 5 }}\r\n      >\r\n        <Close\r\n          className=\"password-reset__close-icon\"\r\n          onClick={closePasswordReset}\r\n        />\r\n\r\n        <img src={Logo} className=\"password-reset__logo\" />\r\n\r\n        <h1>Reset your password</h1>\r\n\r\n        {globalErrors && (\r\n          <div style={{ color: \"red\", transform: \"translateY(170%)\" }}>\r\n            {globalErrors}\r\n          </div>\r\n        )}\r\n\r\n        <InputsWrapper\r\n          inputs={generateResetPasswordInputs(values, errors, touched)}\r\n          eventsFunctions={{\r\n            onChange: handleChange,\r\n            onBlur: handleBlur,\r\n          }}\r\n        />\r\n\r\n        <button\r\n          className=\"password-reset__btn password-reset__btn--margin\"\r\n          type=\"submit\"\r\n          onClick={handleSubmit}\r\n          disabled={resetting || errors.email}\r\n        >\r\n          {resetting ? \"RESETTING...\" : \"RESET\"}\r\n        </button>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default PasswordReset;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\singup\\singup.jsx",["228"],"import React, { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nimport InputsWrapper from \"./../../inputsWrapper/inputsWrapper\";\r\n\r\nimport { handleErrors } from \"../../../../server/firebase/errorHandling\";\r\nimport {\r\n  checkErrors,\r\n  useCustomFormik,\r\n  closeFormWithNoBubbling,\r\n} from \"../helper-functions\";\r\nimport { generateSingupInputs } from \"../inputs-list\";\r\nimport \"./singup.scss\";\r\nimport { ReactComponent as Close } from \"../../../../assets/img/close.svg\";\r\nimport Logo from \"../../../../assets/img/logo.png\";\r\n\r\nconst SingUp = ({ closeSingup, firebase }) => {\r\n  const [registering, setRegistering] = useState(false);\r\n  const [globalErrors, setGlobalErrors] = useState(null);\r\n\r\n  //Formik init\r\n  const {\r\n    handleSubmit,\r\n    touched,\r\n    errors,\r\n    handleChange,\r\n    values,\r\n    handleBlur,\r\n    setErrors,\r\n  } = useCustomFormik(\"singup\", doSubmit);\r\n\r\n  async function doSubmit({ email, password }) {\r\n    setGlobalErrors(null);\r\n    try {\r\n      setRegistering(true);\r\n      const data = await firebase.doCreateUserWithEmailAndPassword(\r\n        email,\r\n        password\r\n      );\r\n      //store the json web token in the localstorage\r\n      localStorage.setItem(\r\n        \"user-authed\",\r\n        JSON.stringify(\r\n          data.user.ya.split(\".\")[0] + \".\" + data.user.ya.split(\".\")[1]\r\n        )\r\n      );\r\n      closeSingup();\r\n    } catch (error) {\r\n      handleErrors(\"singup\", error, setErrors, setGlobalErrors);\r\n    }\r\n    setRegistering(false);\r\n  }\r\n\r\n  return (\r\n    <motion.div\r\n      transition={{ duration: 0.6 }}\r\n      exit={{ opacity: 0 }}\r\n      className=\"singup--background\"\r\n      onClick={e => closeFormWithNoBubbling(e, closeSingup)}\r\n      onKeyPress={e => {\r\n        if (e.key === \"Enter\") handleSubmit();\r\n      }}\r\n    >\r\n      <motion.div\r\n        className=\"singup\"\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        exit={{ y: 5 }}\r\n        transition={{ duration: 0.5 }}\r\n      >\r\n        <Close className=\"singup__close-icon\" onClick={closeSingup} />\r\n\r\n        <img src={Logo} className=\"singup__logo\" />\r\n\r\n        <h1>Become a member</h1>\r\n\r\n        {globalErrors && (\r\n          <div style={{ color: \"red\", transform: \"translateY(170%)\" }}>\r\n            {globalErrors}\r\n          </div>\r\n        )}\r\n\r\n        <InputsWrapper\r\n          inputs={generateSingupInputs(values, errors, touched)}\r\n          eventsFunctions={{ onChange: handleChange, onBlur: handleBlur }}\r\n        />\r\n\r\n        <button\r\n          className=\"singup__btn singup__btn--margin\"\r\n          disabled={registering || checkErrors(errors)}\r\n          onClick={handleSubmit}\r\n        >\r\n          {registering ? \"REGISTERING\" : \"REGISTER\"}\r\n        </button>\r\n      </motion.div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default SingUp;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\fake-db\\db-functions.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\server\\firebase\\errorHandling.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\inputs-list.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\reuseable\\navBar\\helper-functions.js",[],"C:\\Users\\DELL 3510\\Desktop\