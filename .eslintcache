(value)) return;\r\n    }\r\n\r\n    if (name === \"cvv\") {\r\n      if ((value !== \"\" && !numRegx.test(value)) || value.length > 4) return;\r\n    }\r\n\r\n    handleChange(e);\r\n  };\r\n\r\n  const doSubmit = values => {\r\n    console.log(\"submit!!\");\r\n  };\r\n\r\n  return (\r\n    <div className=\"form\">\r\n      <div className=\"form__line\">\r\n        <InputsWrapper\r\n          inputs={generateLine1And2(values, errors, touched)}\r\n          eventsFunctions={{ onChange: customHandleChange, onBlur: handleBlur }}\r\n        />\r\n      </div>\r\n\r\n      <div className=\"form__line\">\r\n        <InputsWrapper\r\n          inputs={generateLine3(values, errors, touched)}\r\n          eventsFunctions={{ onChange: customHandleChange, onBlur: handleBlur }}\r\n        />\r\n      </div>\r\n\r\n      <button className=\"form__submit\" onClick={handleSubmit}>\r\n        Submit\r\n      </button>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Form;\r\n","C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\card\\card.jsx",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\form\\inputsList.js",[],"C:\\Users\\DELL 3510\\Desktop\\Khaled\\code\\projects\\js-react\\E-Commerce app\\e-commerce-app\\src\\components\\main\\order\\checkout\\cardPayement\\form\\yupShema.js",[],{"ruleId":"278","replacedBy":"279"},{"ruleId":"280","replacedBy":"281"},{"ruleId":"282","severity":1,"message":"283","line":13,"column":21,"nodeType":"284","messageId":"285","endLine":13,"endColumn":33},{"ruleId":"282","severity":1,"message":"286","line":25,"column":10,"nodeType":"284","messageId":"285","endLine":25,"endColumn":16},{"ruleId":"287","severity":1,"message":"288","line":67,"column":9,"nodeType":"289","endLine":67,"endColumn":75},{"ruleId":"287","severity":1,"message":"288","line":70